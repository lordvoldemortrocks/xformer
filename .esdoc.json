{
  "source": "./src",
  "destination": "./docs",
  "includes": ["\\palette.js|main.js$"],
  "excludes": ["\\.config\\.js$"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "accessor": { "access": ["public", "protected", "private"], "autoPrivate": true },
        "undocumentIdentifier": { "enable": true },
        "unexportedIdentifier": { "enable": false },
        "typeInference": { "enable": true },
        "brand": {
          "logo": "./logo.png",
          "title": "xFormer",
          "description": "xFormer makes data transformations easy on your cognition and hassle-free.",
          "repository": "https://gitlab.com/muhammadkasim/xformer",
          "site": "https://www.npmjs.com/package/@muhammadkasim/xformer",
          "author": "Muhammad Kasim",
          "image": "./logo.png"
        },
        "test": {
          "source": "./__tests__",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.js$"],
          "excludes": ["\\.config\\.js$"]
        }
      }
    }
  ]
}
