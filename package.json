{
  "name": "@muhammadkasim/xformer",
  "version": "0.1.0-alpha-2.0",
  "description": "xFormer provides an extensive set of utilities, a way to define declarative transformation pipelines and a parser to facilitate easier data transformations.",
  "main": "lib/index.js",
  "author": "eMumba <muhammad.kasim@emumba.com>",
  "repository": "https://gitlab.com/muhammadkasim/xformer",
  "license": "MIT",
  "scripts": {
    "build": "./node_modules/.bin/esdoc && babel src --out-dir lib --quiet",
    "test": "jest --watch"
  },
  "keywords": [
    "ramda",
    "functional",
    "pure"
  ],
  "files": [
    "lib"
  ],
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "verbose": true,
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "babel": {
    "presets": [
      "stage-0"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "ramda": "0.26.0"
  },
  "devDependencies": {
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^23.6.0"
  }
}
